{
    "uarch": [
        "-collect", "uarch-exploration",
        "-knob", "collect-memory-bandwidth=true",
        "-knob", "pmu-collection-mode=detailed"
    ],
    "prefetches": [
        "-collect-with", "runsa",
        "-knob", "event-config=MEM_INST_RETIRED.ALL_LOADS,MEM_LOAD_RETIRED.L1_HIT,MEM_LOAD_RETIRED.L1_MISS,MEM_LOAD_RETIRED.L2_HIT,MEM_LOAD_RETIRED.L2_MISS,MEM_LOAD_RETIRED.L3_HIT,MEM_LOAD_RETIRED.L3_MISS,LOAD_HIT_PREFETCH.SWPF,L1D.HWPF_MISS,L2_RQSTS.SWPF_HIT,L2_RQSTS.ALL_HWPF,L2_LINES_OUT.USELESS_HWPF,SW_PREFETCH_ACCESS.NTA,SW_PREFETCH_ACCESS.T0,SW_PREFETCH_ACCESS.T1_T2,SW_PREFETCH_ACCESS.PREFETCHW,TOPDOWN.SLOTS,TOPDOWN.MEMORY_BOUND_SLOTS,CYCLE_ACTIVITY.STALLS_L1D_MISS,CYCLE_ACTIVITY.STALLS_L2_MISS,CYCLE_ACTIVITY.STALLS_L3_MISS,OFFCORE_REQUESTS.DATA_RD,OFFCORE_REQUESTS.DEMAND_DATA_RD, OFFCORE_REQUESTS.L3_MISS_DEMAND_DATA_RD,CYCLE_ACTIVITY.STALLS_TOTAL,CYCLE_ACTIVITY.CYCLES_MEM_ANY,LONGEST_LAT_CACHE.MISS,LONGEST_LAT_CACHE.REFERENCE,MEM_BOUND_STALLS.LOAD_L2_HIT,MEM_BOUND_STALLS.LOAD_LLC_HIT,MEM_BOUND_STALLS.LOAD_DRAM_HIT,MEM_BOUND_STALLS.LOAD",
        "-knob", "dram-bandwidth-limits=true",
        "-knob", "max-region-duration=0.001",
        "-knob", "sampling-interval=0.01",
        "-knob", "uncore-sampling-interval=1",
        "-knob", "analyze-openmp=true"
    ]
}
